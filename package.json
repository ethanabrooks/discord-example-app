{
  "name": "gpt-game",
  "version": "1.0.0",
  "description": "Discord example app",
  "main": "app.js",
  "type": "module",
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "build": "tsc --build",
    "clean": "tsc --build --clean",
    "dev": "nodemon --exec \"tsc --build && node dist/app.js\"",
    "register": "node dist/commands.js",
    "start": "tsc --build && node dist/app.js",
    "watch-ts": "tsc -w",
    "watch-node": "nodemon dist/app.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold\" \"npm:watch-ts\" \"npm:watch-node\""
  },
  "author": "Ethan Brooks",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^20.3.1",
    "@dqbd/tiktoken": "^1.0.7",
    "canvas": "^2.11.2",
    "discord-interactions": "^3.2.0",
    "discord.js": "^14.11.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "node-fetch": "^3.3.0",
    "openai": "^3.3.0",
    "pino": "^8.14.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "pino-pretty": "^10.0.1",
    "@types/pino": "^7.0.5",
    "nodemon": "^2.0.22",
    "prettier": "^3.0.0",
    "typescript": "^5.1.3",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1"
  },
  "repository": "git@github.com:ethanabrooks/discord-example-app.git"
}
